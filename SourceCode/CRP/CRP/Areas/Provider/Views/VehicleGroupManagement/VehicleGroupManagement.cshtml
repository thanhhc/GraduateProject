
@{
    ViewBag.Title = "Quản lý nhóm giá";
    Layout = "~/Views/Shared/_ManagementLayout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <h2>Quản lý nhóm giá</h2>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="ibox ibox-content float-e-margins">
        <div class="row">
            <div class="pull-right m-r m-l m-b">
                <button class="btn btn-primary" id="btnAddVehiclePopup"><i class="fa fa-plus"></i> Thêm nhóm mới</button>
            </div>
        </div>

        <table class="table table-striped table-bordered table-hover " id="garages" width="100%">
        </table>
    </div>
</div>
<div id="myModal" class="modal fade" role="dialog"></div>

<div class="modal fade" id="mdModal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
        </div>
    </div>
</div>

    @{ BundleTable.Bundles.Add(new StyleBundle("~/Styles/VehicleGroupManagement")
                            .Include("~/Content/awesome-bootstrap-checkbox.css",
                                "~/Content/chosen.css",
                                "~/Content/DataTables/css/dataTables.bootstrap.css",
                                "~/Content/pageStyle/vehicleGroupManagement.css"));
    }
    @section Styles {
        @Styles.Render("~/Styles/VehicleGroupManagement")
    }
    @{ BundleTable.Bundles.Add(new ScriptBundle("~/Scripts/VehicleGroupManagement")
                             .Include("~/Scripts/chosen.jquery.js",
                                "~/Scripts/DataTables/jquery.dataTables.js",
                                "~/Scripts/DataTables/dataTables.bootstrap.js",
                                "~/Scripts/utils/generateUIElements.js",
                                "~/Scripts/utils/customModals.js",
                                "~/Scripts/utils/generateCustomDataTablesFilters.js",
                                "~/Scripts/pageScript/vehicleGroupManagement.js"));
    }
    @section Scripts {
        @Scripts.Render("~/Scripts/VehicleGroupManagement")
    }
